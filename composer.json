{
    "name": "experteam/api-redis-bundle",
    "description": "REDIS functionality bundle for APIs.",
    "homepage": "https://github.com/experteam-mx/api-redis-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Experteam",
            "email": "info@experteam.com.ec"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "doctrine/orm": "^2.0",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/dependency-injection": "5.2.*|5.3.*|5.4.*",
        "symfony/http-kernel": "5.2.*|5.3.*|5.4.*",
        "symfony/serializer": "5.2.*|5.3.*|5.4.*",
        "symfony/messenger": "5.2.*|5.3.*|5.4.*",
        "snc/redis-bundle": "^3.2",
        "predis/predis": "^1.1",
        "experteam/api-base-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Experteam\\ApiRedisBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Experteam\\ApiRedisBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/flex": "^1.11",
        "symfony/dotenv": "5.2.*|5.3.*",
        "symfony/phpunit-bridge": "5.2.*|5.3.*",
        "symfony/config": "5.2.*|5.3.*",
        "symfony/property-access": "5.2.*|5.3.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
