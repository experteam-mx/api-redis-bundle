{
    "name": "experteam/api-redis-bundle",
    "description": "REDIS functionality bundle for APIs.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Experteam",
            "email": "info@experteam.com.ec"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/orm": "2.7.*",
        "doctrine/doctrine-bundle": "2.0.*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "symfony/config": "5.1.*",
        "symfony/dependency-injection": "5.1.*",
        "symfony/http-kernel": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/messenger": "5.1.*",
        "predis/predis": "^1.1",
        "symfony/flex": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Experteam\\ApiRedisBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Experteam\\ApiRedisBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.1.*"
    }
}
